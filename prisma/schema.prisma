// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?

  @@map("user")
}

model Category {
  id          String    @id @default(uuid())
  name        String
  description String
  created_at  DateTime?
  updated_at  DateTime?

  @@map("category")
}
